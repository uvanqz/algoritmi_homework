class Solution(object):
    def removeOuterParentheses(self, s): 
        c = [] #создаём пустой список
        i = 0
        otvet = "" #создаём пустую строку, в которую будет вноситься итоговый s
        while i < len(s): #цикл, выолняющийся, пока i < длины всех элементов
            if s[i] == '(':
                c.append(i) #элемент добавляется в конец списка
            if s[i] == ')':
                a = c.pop() #удаление последнего элемента из списка c
            if not c: #если список пустой
                otvet += s[a+1 : i] 
            i += 1 #i увеличивается на 1
        return otvet

##Сложность данной программы O(n*log(n)).
