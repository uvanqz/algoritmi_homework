class Solution:
    def tribonacci(self, n: int) -> int:
        trib = { 0: 0, 1 : 1, 2 : 1} #создаём словарь, в который будут вноситься пары ключ-значение (ключ - n, значение - T от n), добавляем туда пары от 0 до 2
        if n in trib: #если ключ n есть в словаре
            return trib[n] #выводим соответствующее значение
        for n in range(3, n + 1): #для всех последующих n от 3 до n+1
            trib[n] = trib[n - 3] + trib[n - 2] + trib[n - 1] #вычисляем T от n по формуле
        return trib[n] 

#Сложность функции O(n)
